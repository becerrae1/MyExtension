{
    "name": "pybin",
    "displayName": "Pybin",
    "description": "Install this extension to have a familiar face help you traverse your new venture with live code examples from Spongebob and the Crew!",
    "version": "1.0.0",
    "publisher": "Ethan",
    "icon": "SpongebobFunnyFace.jpg",
    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "python",
                "path": "./snippets/snippets.code-snippets"
            }
            {
                "language": "javascript",
                "path": "./snippets/snippets.code-snippets"
            }
            {
                "language": "javascriptreact",
                "path": "./snippets/snippets.code-snippets"
            }
        ]
    }
}
